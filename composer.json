{
    "name": "symplely/uv-ffi",
    "description": " An Foreign Function interface (FFI) of libuv cross-platform event-driven asynchronous I/O library.",
    "keywords": [
        "ffi",
        "ext-uv",
        "event-driven",
        "parallel",
        "thread",
        "uv_spawn",
        "libuv",
        "async",
        "cross-platform",
        "asynchronous"
    ],
    "homepage": "https://github.com/symplely/uv-ffi",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lawrence Stubbs",
            "email": "lstubbs@techno.express"
        }
    ],
    "require": {
        "php": ">7.4",
        "mnsami/composer-custom-directory-installer": "^2"
    },
    "autoload": {
        "files": [
            "preload.php",
            "ffi/UVConstants.php",
            "ffi/UVFunctions.php"
        ],
        "classmap": [
            "ffi/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^7 | ^8"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --bootstrap vendor/autoload.php tests"
    },
    "extra": {
        "installer-paths": {
            "./cdef": [
                "symplely/uv-ffi"
            ]
        }
    }
}
